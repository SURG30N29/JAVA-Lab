import java.util.Scanner;
class factorial{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n,count,fact=1;
        System.out.println("Enter a number: ");
        n = sc.nextInt();
        count=n;
        while(count>0){
            fact=fact*count;
            count--;
        }
        System.out.println("The factorial of "+n+" is "+fact);
    }
}
